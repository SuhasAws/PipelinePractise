pipeline {
  
  agent any 
    
    stages {
      
      stage("Build") {
        
        steps {
          
          echo " This is output of Build"
          
        }
        
      }
      
      stage("Test") {
        
        steps {
          
          echo " This is output of Test"
          
        }
        
      }
      
      stage("deploy") {
        
        steps {
          
          echo " This is output step of Deploy"
          
        }
        
  }

    }

}
      

    
  
